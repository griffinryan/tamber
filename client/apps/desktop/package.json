{
  "name": "@timbre/desktop",
  "version": "0.0.1",
  "private": true,
  "main": "src/main.tsx",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently \"yarn dev\" \"wait-on http://localhost:5173\" \"yarn electron:start\"",
    "electron:start": "yarn compile:electron && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.js",
    "compile:electron": "tsc -p electron/tsconfig.json",
    "build": "vite build",
    "build:desktop": "yarn compile:electron && yarn build && electron-builder --dir",
    "preview": "vite preview",
    "lint": "echo \"TODO: lint\"",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "echo \"TODO: tests\""
  },
  "dependencies": {
    "@timbre/api": "0.0.1",
    "@timbre/config": "0.0.1",
    "@timbre/ui": "0.0.1",
    "@timbre/state": "0.0.1",
    "@tamagui/button": "^1.98.3",
    "@tamagui/colors": "^1.98.3",
    "@tamagui/core": "^1.98.3",
    "@tamagui/font-inter": "^1.98.3",
    "@tamagui/font-silkscreen": "^1.98.3",
    "@tamagui/helpers": "^1.98.3",
    "@tamagui/shorthands": "^1.98.3",
    "@tamagui/stacks": "^1.98.3",
    "@tamagui/text": "^1.98.3",
    "@tamagui/theme": "^1.98.3",
    "@tamagui/input": "^1.98.3",
    "@tamagui/linear-gradient": "^1.98.3",
    "react-native-reanimated": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.4",
    "react-native-web": "^0.19.10",
    "electron": "^29.1.0",
    "electron-builder": "^24.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@tamagui/vite-plugin": "^1.98.3",
    "react-native-svg": "^14.1.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
