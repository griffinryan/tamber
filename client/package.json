{
  "name": "@timbre/client",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "yarn install --check-files",
    "dev:desktop": "yarn workspace @timbre/desktop dev",
    "dev:desktop:electron": "yarn workspace @timbre/desktop dev:electron",
    "build:desktop": "yarn workspace @timbre/desktop build:desktop",
    "lint": "yarn workspaces run lint",
    "typecheck": "yarn workspaces run typecheck",
    "test": "yarn workspaces run test"
  },
  "packageManager": "yarn@1.22.22"
}
